                                        if (names[i] == 'idEstablecimiento') {
                                            $("#" + names[i]).val(data[0][names[i]]);

                                        }
                                        if (names[i] == 'idCursos') {
                                            curso = data[0][names[i]];

                                        }


                                        //Verificamos cuales son checkbox
                                        if ($("#" + names[i]).attr('type') == 'checkbox') {
                                            if (data[0][names[i]] == 1) {
                                                $("#" + names[i]).attr('checked', 'checked')

                                            }

                                        } else {
                                            $("#" + names[i]).val(data[0][names[i]]);
                                        }


                                    }

                                }

                                $("#idEstablecimiento").trigger("change", [curso]);

                                $("#comuna").trigger("liszt:updated")
