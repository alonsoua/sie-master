        //Guardamos los datos

        $('#guardar').click(function (e) {

            var elementos = $("#matricula").find('input,select,checkbox');
            var datos;
            var datos = {};

            for (var i = 0; i < elementos.length; i++) {
                if (elementos[i].getAttribute("id") != null) {
                    if (elementos[i].type == "checkbox") {
                        if (elementos[i].checked == true) {
                            datos[elementos[i].getAttribute("id")] = 1;
                        } else {
                            datos[elementos[i].getAttribute("id")] = 2;
                        }


                    } else {
                        datos[elementos[i].getAttribute("id")] = elementos[i].value;
                    }

                }

            }

            if ($('#rutAlumno').hasClass('errorinput') || $('#rutApoderado').hasClass('errorinput')) {

                const errorss = Swal.mixin({
                    toast: true,
                    position: 'top',
                    showConfirmButton: false,
                    timer: 3000
                });

                errorss({
                    type: 'error',
                    title: 'El RUT no es válido'
                })

                return false;

            } else {

                $.ajax({
                    cache: false,
                    async: true,
                    dataType: 'json',
                    type: 'POST',
                    contentType: 'application/x-www-form-urlencoded',
                    url: 'guardamatricula',
                    data: JSON.stringify(datos),
                    success: function (data) {
                        if (parseInt(data.response) === 1) {

                            swal({
                                title: "Guardado",

                                type: "success"
                            }).then(okay => {
                                if (okay) {
                                    window.location.href = data.url;
                                }
                            });

                        } else if (parseInt(data.response) === 0) {
                            switch (parseInt(data.status)) {
                                case 1:
                                    const errors = Swal.mixin({
                                        toast: true,
                                        position: 'top',
                                        showConfirmButton: false,
                                        timer: 3000
                                    });

                                    errors({
                                        type: 'error',
                                        title: 'Reinicie su Sesión '
                                    })
                                    break;
                                case 2:
                                    const error = Swal.mixin({
                                        toast: true,
                                        position: 'top',
                                        showConfirmButton: false,
                                        timer: 3000
                                    });

                                    error({
                                        type: 'error',
                                        title: 'Error al ingresar los datos '
                                    })
                                    break;

                                case 3:
                                    const errorss = Swal.mixin({
                                        toast: true,
                                        position: 'top',
                                        showConfirmButton: false,
                                        timer: 3000
                                    });

                                    errorss({
                                        type: 'error',
                                        title: 'Complete todos los campos (*)'
                                    })

                                    break;


                            }
                        } else {
                            const error = Swal.mixin({
                                toast: true,
                                position: 'top',
                                showConfirmButton: false,
                                timer: 3000
                            });

                            error({
                                type: 'error',
                                title: 'Error al ingresar los datos '
                            })
                        }
                    },
                    error: function () {
                        const error = Swal.mixin({
                            toast: true,
                            position: 'top',
                            showConfirmButton: false,
                            timer: 3000
                        });

                        error({
                            type: 'error',
                            title: 'Error al ingresar los datos '
                        })
                    }
                });

            }


            e.stopImmediatePropagation();
        });


    });


</script>
<script type="text/javascript">
    $(function () {
        $("#foto").on("change", function () {
            var files = !!this.files ? this.files : [];
            if (!files.length || !window.FileReader) return; // no file selected, or no FileReader support

            if (/^image/.test(files[0].type)) { // only image file
                var reader = new FileReader(); // instance of the FileReader
                reader.readAsDataURL(files[0]); // read the local file

                reader.onloadend = function () { // set image data as background of div
                    $("#imagePreview").css("background-image", "url(" + this.result + ")");
                }
            }
        });
    });
</script>
